{
    "name": "silvercommerce/discounts",
    "description": "Module that adds discounts (either as a code, or on a group) to SilverCommerce",
      "type": "silverstripe-vendormodule",
      "homepage": "http://github.com/silvercommerce/discounts",
      "keywords": [
          "silverstripe",
          "silvercommerce",
          "commerce",
          "ecommerce",
          "discounts",
          "discount codes"
      ],
      "license": "BSD-3-Clause",
      "authors": [
          {
              "name": "Morven Lewis-Everley",
              "email": "morven@ilateral.co.uk"
          },
          {
              "name": "Mark Anderson",
              "email": "mark@ilateral.co.uk"
          }
      ],
      "support": {
          "issues": "http://github.com/silvercommerce/discounts/issues"
      },
      "require": {
          "silverstripe/framework": "^5",
          "silvercommerce/settings": "dev-ss5",
          "silvercommerce/orders-admin": "dev-ss5",
          "symbiote/silverstripe-gridfieldextensions": "^4"
      },
      "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-orders-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-settings"
        }
    ],
      "autoload": {
          "psr-4": {
              "SilverCommerce\\Discounts\\": "src/",
              "SilverCommerce\\Discounts\\Tests\\": "tests/"
          }
      },
      "scripts": {
          "lint": "vendor/bin/phpcs src/ tests/",
          "lint-clean": "vendor/bin/phpcbf src/ tests/"
      },
      "minimum-stability": "dev",
      "prefer-stable": true
  }
  
