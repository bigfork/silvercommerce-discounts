{
    "name": "silvercommerce/discounts",
    "description": "Module that adds discounts (either as a code, or on a group) to SilverCommerce",
      "type": "silverstripe-vendormodule",
      "homepage": "http://github.com/silvercommerce/discounts",
      "keywords": [
          "silverstripe",
          "silvercommerce",
          "commerce",
          "ecommerce",
          "discounts",
          "discount codes"
      ],
      "license": "BSD-3-Clause",
      "authors": [
          {
              "name": "Morven Lewis-Everley",
              "email": "morven@ilateral.co.uk"
          },
          {
              "name": "Mark Anderson",
              "email": "mark@ilateral.co.uk"
          }
      ],
      "support": {
          "issues": "http://github.com/silvercommerce/discounts/issues"
      },
      "require": {
          "silverstripe/framework": "~4.0",
          "silvercommerce/settings": "~1.0",
          "silvercommerce/orders-admin": "~1.0"
      },
      "extra": {
          "expose": [
              "client/dist"
          ]
      },
      "autoload": {
          "psr-4": {
              "SilverCommerce\\Discounts\\": "src/",
              "SilverCommerce\\Discounts\\Tests\\": "tests/"
          }
      }
  }
  